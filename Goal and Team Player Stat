                 Console.WriteLine($"\nlast_scoreteam: {response["last_score"]["point_amount"]}");
                 if (response["last_score"]["goal_type"].ToString() != "[NO GOAL]") { Console.WriteLine($"\nlast_scoregoal_type: {response[""]}\n>>Scored by {response["last_score"]["person_scored"]}>>Thrown at {response["last_score"]["disc_speed"]}m/s from {response["last_score"]["distance_thrown"]}m away."); }
                 Console.WriteLine($"\nteamsplayersstats: {response["teams"][0]["stats"]}");
                 Console.WriteLine($"\nteamsstatspoints: {response["teams"][0]["stats"]["points"]}>>Points\n {response["teams"][0]["stats"]["possession_time"]}>>Possession Time\n {response["teams"][0]["stats"]["interceptions"]}>>Interceptions\n {response["teams"][0]["stats"]["blocks"]}>>Blocks\n {response["teams"][0]["stats"]["steals"]}>>Steals\n {response["teams"][0]["stats"]["catches"]}>>Catches\n {response["teams"][0]["stats"]["passes"]}>>Passes\n {response["teams"][0]["stats"]["saves"]}>>Saves\n {response["teamsstatsgoals"]}>>Goals\n {response["teams"][0]["stats"]["stuns"]}>>Stuns\n {response["teams"][0]["stats"]["assists"]}>>Assists\n"); 
